<%= form_for @meeting do |form| %>
  <div class="form-group">
    <%= form.label :meeting_time %>
    <%= form.text_field :meeting_time, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :firstyear %>
    <%= form.text_field :firstyear, class: "form-control" %>
  </div>
  
   <div class="form-group">
    <%= form.label :secondyear %>
    <%= form.collection_select :secondyear, SecondYear.all, :id, :first_name, {}, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :case_id %>
    <%= form.collection_select :case_id, Case.all, :id, :title, {}, class: "form-control" %>
  </div>
  <%= form.button 'Create Meeting', class: "btn btn-success" %>
<% end %>